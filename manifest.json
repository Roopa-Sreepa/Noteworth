{
  "name": "Web annotation",
  "version": "1.0", 
  "description": "Annotating the web.",
  "permissions": ["tabs"],
  "browser_action": 
  {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["contentscript.js"]
  }],
  "background": { "scripts": ["background.js"] },
  "manifest_version": 2
}
